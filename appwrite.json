{
  "projectId": "ogbanana",
  "functions": [
    {
      "$id": "gen-ogp",
      "name": "generateOgpData",
      "runtime": "node-18.0",
      "specification": "s-1vcpu-1gb",
      "execute": ["any"],
      "events": [],
      "scopes": ["users.read", "users.write"],
      "schedule": "",
      "timeout": 15,
      "enabled": true,
      "logging": true,
      "entrypoint": "src/main.js",
      "commands": "apk update && apk fetch chromium nss freetype harfbuzz ca-certificates ttf-freefont && npm install",
      "path": "functions/generateOgpData"
    },
    {
      "$id": "limit-resetter",
      "name": "limit-resetter",
      "runtime": "bun-1.1",
      "specification": "s-0.5vcpu-512mb",
      "execute": ["any"],
      "events": [],
      "scopes": ["users.read", "users.write"],
      "schedule": "0 0 1 * *",
      "timeout": 15,
      "enabled": true,
      "logging": true,
      "entrypoint": "src/main.ts",
      "commands": "bun install",
      "path": "functions/limit-resetter"
    }
  ]
}
